import java.util.Random;

public class test {
	public static void main(String[] args) {
		int[][] a={{200,300},{6,8},{9,12},{555,444}};
		Random r=new Random();
		while (true) {
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
			System.out.println(a[r.nextInt(4)][0]+a[r.nextInt(4)][1]);
		}
	}
}
